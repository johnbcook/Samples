		html, body 		{ margin: 0; padding: 0; }
		body 			{ color: #333; font: 12px Helvetica, Arial, sans-serif; line-height: 18px; }
		h2				{ color: #333; }
		a				{ color: #337810; }
		p				{ margin: 0 0 18px; }
		#wrapper		{ float: left; width: 100%; }
		#wrapper1       { float: left; width: 80%; }
		
		.signup_btn {
						cursor : pointer;
						margin : 10px 0 -20px 0;
						align  : center;
						text-align : center;
					}
					
		.form-noindent {
						background-color : #ffffff;
						border : 1px solid #c3d9ff;				
						
					  }
	    #login    { float: left; border: 1px solid #c3d9ff; padding: 10px 18px 9px;  text-align: left; background: #E8EEFA; height: 135px; width: 200x; }	  
		#signin     { float: right; top; border: 1px solid #c3d9ff; padding: 10px 18px 9px;  text-align: left; background: #E8EEFA; height: 135px; width: 120px; }	
		#account    { float: left; border: 1px solid #c3d9ff; padding: 10px 18px 9px;  text-align: left; background: #E8EEFA; height: 220px; width: 200x; }
				  
