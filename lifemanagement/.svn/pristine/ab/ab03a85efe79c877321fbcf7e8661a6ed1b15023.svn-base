
body, td, div, p, a, font, span {
font-family : arial, sans-serif;
}
.c {
height : 0;
}
.bubble {
background-color : #c3d9ff;
}
.tl {
padding : 0;
text-align : left;
vertical-align : top;
}
.tr {
padding : 0;
text-align : right;
vertical-align : top;
}
.bl {
padding : 0;
text-align : left;
vertical-align : bottom;
}
.br {
padding : 0;
text-align : right;
vertical-align : bottom;
}
.caption {
color : #000000;
white-space : nowrap;
background : #e8eefa;
text-align : center;
}
.form-noindent {
background-color : #ffffff;
border : 1px solid #c3d9ff;
}
.feature-image {
vertical-align : top;
text-align : right;
}
.feature-description {
vertical-align : top;
text-align : left;
}
.signup_btn {
cursor : pointer;
margin : 10px 0 -20px 0;
text-align : center;
}
.signup_btn_link {
color : #000;
text-align : center;
text-align : center;
text-decoration : none;
padding : 0 7px;
font-weight : bold;
font-size : 14px;
white-space : nowrap;
}
